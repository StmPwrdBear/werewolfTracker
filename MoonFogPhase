var moonPhase = 'X'; 
var foggyNight = true | false 
if (moonPhase === 'full' && || foggyNight) {console.log('Howwwwlll!!');} 
switch (moonPhase) { 
 case 'full': 
    console.log('Howwwwlll!!'); 
    break; 
 case 'mostly full': 
    console.log('Arms and legs are getting harrier'); 
    break; 
 case 'mostly new': 
    console.log('Back on two feet'); 
    break; 
  default: 
    console.log('Invalid moon phase'); 
    break; 
}
switch (foggyNight) {
case 'true':
  console.log('Arms and legs are getting hairier');
  
  case 'false':
    console.log('Back on two feet');
    }
